{
  "name": "package-seed-lib",
  "version": "1.0.0",
  "description": "publish ts lib",
  "license": "MIT",
  "repository": "",
  "keywords": [],
  "author": "",
  "main": "lib/index.js",
  "unpkg": "dist/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./lib ./dist ./es ./coverage ./types",
    "lint": "eslint --ext js,ts src tests",
    "check-types": "tsc --build tsconfig.lint.json",
    "dev": "rollup -w -c",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --watch --config jest.config.json",
    "test:coverage": "jest --coverage --config jest.config.json",
    "build": "rollup -c",
    "pretest": "npm run build",
    "prepublishOnly": "npm run clean && npm run check-types && npm run lint && npm test"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^16.4.10",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "glob": "^7.1.6",
    "husky": "4.2.5",
    "jest": "^27.0.0",
    "lint-staged": "^12.0.2",
    "netlify-plugin-cache": "^1.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rxjs": "^7.3.0",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "eslint -ext ts,js",
      "tsc --noEmit --allowJs"
    ],
    "tests/**/*": [
      "eslint -ext ts,js",
      "tsc --noEmit --allowJs"
    ]
  }
}
